{
    "root": "public/",
    "headers": {
      "/**": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "/**.css": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/**.js": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/static/**": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/icons/*.png": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "routes": {
      "/assets/*": "/assets/",
      "/**": "index.html",
      "/pages/**": "/sety",
      "/": "/graphql"
      }, 
      "redirects": {
        "/old/gone/": {
          "url": "/404",
          "status": 404
        }
      },
    "https_only": true,
    "error_page": "/pages/404.js",
    "clean_urls": true
  }